  "repository": {
    "type": "git",
    "url": "https://github.com/Rishu22889/ai-apply.git"
  },
  "bugs": {
    "url": "https://github.com/Rishu22889/ai-apply/issues"
  },
  "homepage": "https://github.com/Rishu22889/ai-apply#readme",
