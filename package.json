{
  "name": "ai-apply",
  "version": "1.0.0",
  "description": "AI-Powered Job Application System - Automate your job search with intelligent matching",
  "main": "run.py",
  "scripts": {
    "setup": "chmod +x setup.sh && ./setup.sh",
    "dev": "concurrently \"python run.py\" \"cd frontend && npm run dev\"",
    "build": "cd frontend && npm run build",
    "start": "python run.py",
    "frontend": "cd frontend && npm run dev",
    "backend": "python run.py",
    "sandbox": "python sandbox/job_portal.py",
    "test": "pytest",
    "test:frontend": "cd frontend && npm test"
  },
  "keywords": [
    "ai",
    "job-application",
    "automation",
    "machine-learning",
    "react",
    "fastapi",
    "python",
    "javascript",
    "career",
    "recruitment"
  ],
